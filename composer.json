{
    "name": "prosperworks/php-sdk-v1",
    "description": "Prosperworks PHP SDK",
    "keywords": ["prosperworks", "api", "sdk"],
    "type": "library",
    "homepage": "https://github.com/radoneman/prosperworks-php-sdk",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "radoneman",
            "email": "radone@gmail.com",
            "homepage": "https://github.com/radoneman"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.8",
        "guzzlehttp/guzzle": "~5.0"
    },
    "suggest": {
        "guzzlehttp/guzzle": "Allows for implementation of the Guzzle HTTP client"
    },
    "autoload": {
        "psr-4": {
            "Prosperworks\\": "src/Prosperworks/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Prosperworks\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
